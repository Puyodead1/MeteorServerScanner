{
  "schemaVersion": 1,
  "id": "serverscanner",
  "version": "${version}",
  "name": "ServerScanner",
  "description": "ServerScanner fork for cornbreads server scanner",
  "authors": [
    "Puyodead1",
    "DAMCraft"
  ],
  "icon": "assets/serverscanner/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.puyodead1.serverscanner.ServerScanner"
    ]
  },
  "mixins": [
    "serverscanner.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "153,45,34",
    "github:sha": "${gh_hash}"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ">=1.21"
  },
  "recommends": {
    "meteor-client": "*"
  }
}
